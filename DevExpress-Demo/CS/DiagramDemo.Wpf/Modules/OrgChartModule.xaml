                                               Style="{DynamicResource {dxdiagt:DesignerControlCommandsThemeKeys ResourceKey=PrintCommandItemStyle}}" />
                            <dxb:BarButtonItem Command="{Binding Path=Commands.QuickPrint, ElementName=diagramControl}"
