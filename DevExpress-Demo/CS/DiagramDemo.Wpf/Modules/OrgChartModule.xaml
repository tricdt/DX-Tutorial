            <dxr:RibbonControl.ToolbarItems>
                <dxb:BarButtonItem Command="{Binding Path=Commands.Undo, ElementName=diagramControl}"
                                   Style="{DynamicResource {dxdiagt:DesignerControlCommandsThemeKeys ResourceKey=UndoCommandItemStyle}}" />
                <dxb:BarButtonItem Command="{Binding Path=Commands.Redo, ElementName=diagramControl}"
                                   Style="{DynamicResource {dxdiagt:DesignerControlCommandsThemeKeys ResourceKey=RedoCommandItemStyle}}" />
            </dxr:RibbonControl.ToolbarItems>
            <dxr:RibbonControl.ApplicationMenu>
                <dxr:ApplicationMenu>
                    <dxb:BarButtonItem Command="{Binding Path=Commands.OpenFile, ElementName=diagramControl}"
                                       Style="{DynamicResource {dxdiagt:DesignerControlCommandsThemeKeys ResourceKey=OpenFileCommandItemStyle}}" />
                    <dxb:BarButtonItem Command="{Binding Path=Commands.SaveFile, ElementName=diagramControl}"
                                       Style="{DynamicResource {dxdiagt:DesignerControlCommandsThemeKeys ResourceKey=SaveFileCommandItemStyle}}" />
