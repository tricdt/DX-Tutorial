                        <dxb:BarButtonItem Command="{Binding Path=Commands.TreeLayout, ElementName=diagramControl}"
                                           Style="{DynamicResource {dxdiagt:DesignerControlCommandsThemeKeys ResourceKey=TreeLayout_UpCommandItemStyle}}" />
                        <dxb:BarButtonItem Command="{Binding Path=Commands.TreeLayout, ElementName=diagramControl}"
                                           Style="{DynamicResource {dxdiagt:DesignerControlCommandsThemeKeys ResourceKey=TreeLayout_RightCommandItemStyle}}" />
