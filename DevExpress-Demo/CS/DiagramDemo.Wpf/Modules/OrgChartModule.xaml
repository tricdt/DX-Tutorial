                        <dxr:RibbonGalleryBarItem.Gallery>
                            <dxb:Gallery FirstCheckedItem="{Binding Path=RootToolsModel[Theme], ElementName=diagramControl, Mode=TwoWay}"
                                         ColCount="4"
                                         Style="{StaticResource themeGalleryStyle}">
                                <dxb:GalleryItemGroup ItemsSource="{x:Static dxdiag:ThemeRegistrator.Themes}"
                                                      ItemTemplate="{StaticResource themeGalleryControlItemTemplate}" />
                            </dxb:Gallery>
                        </dxr:RibbonGalleryBarItem.Gallery>
                        <dxr:RibbonGalleryBarItem.DropDownGallery>
                            <dxb:Gallery FirstCheckedItem="{Binding Path=RootToolsModel[Theme], ElementName=diagramControl, Mode=TwoWay}"
                                         Style="{StaticResource themeGalleryStyle}">
                                <dxb:GalleryItemGroup ItemsSource="{x:Static dxdiag:ThemeRegistrator.Themes}"
                                                      ItemTemplate="{StaticResource themeGalleryControlItemTemplate}" />
                            </dxb:Gallery>
                        </dxr:RibbonGalleryBarItem.DropDownGallery>
                    </dxr:RibbonGalleryBarItem>
                </dxr:RibbonPageGroup>
                <dxr:RibbonPageGroup Caption="Templates">
                    <dxr:RibbonGalleryBarItem Content="Templates">
                        <dxr:RibbonGalleryBarItem.Gallery>
                            <dxb:Gallery FirstCheckedItem="{Binding SelectedTemplate, Mode=TwoWay}" ColCount="5" Style="{StaticResource templateGalleryStyle}">
                                <dxb:GalleryItemGroup ItemsSource="{Binding Templates}" ItemTemplate="{StaticResource templateGalleryControlItemTemplate}" />
                            </dxb:Gallery>
                        </dxr:RibbonGalleryBarItem.Gallery>
                        <dxr:RibbonGalleryBarItem.DropDownGallery>
                            <dxb:Gallery FirstCheckedItem="{Binding SelectedTemplate, Mode=TwoWay}" ColCount="5" Style="{StaticResource templateGalleryStyle}">
                                <dxb:GalleryItemGroup ItemsSource="{Binding Templates}" ItemTemplate="{StaticResource templateGalleryControlItemTemplate}" />
                            </dxb:Gallery>
                        </dxr:RibbonGalleryBarItem.DropDownGallery>
                    </dxr:RibbonGalleryBarItem>
                </dxr:RibbonPageGroup>
