                                               Style="{DynamicResource {dxdiagt:DesignerControlCommandsThemeKeys ResourceKey=ExportToSvgCommandItemStyle}}" />
                            <dxb:BarButtonItem Command="{Binding Path=Commands.ExportToPdf, ElementName=diagramControl}"
                                               Style="{DynamicResource {dxdiagt:DesignerControlCommandsThemeKeys ResourceKey=ExportToPdfCommandItemStyle}}" />
