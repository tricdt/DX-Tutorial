                        </dxb:PopupMenu>
                    </dxb:BarSplitButtonItem>
                    <dxb:BarSplitButtonItem Command="{Binding Path=Commands.ExportDiagram, ElementName=diagramControl}"
                                            Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=ExportAsStyle}}">
                        <dxb:PopupMenu>
