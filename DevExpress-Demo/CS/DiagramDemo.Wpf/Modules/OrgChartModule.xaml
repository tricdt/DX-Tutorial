    <local:DiagramDemoModule.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/DiagramDemo;component/Resources/OrgChartModuleResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <Style x:Key="diagramContainerStyle" TargetType="{x:Type dxdiag:DiagramContainer}">
                <Setter Property="Anchors" Value="Left, Top"/>
                <Setter Property="ConnectionPoints" Value="0,0.5 0.5,0 1,0.5 0.5,1"/>
                <Setter Property="CanAddItems" Value="False"/>
                <Setter Property="DragMode" Value="ByAnyPoint"/>
                <Setter Property="Height" Value="190"/>
                <Setter Property="Width" Value="320"/>
                <Setter Property="Padding" Value="0"/>
                <Setter Property="ItemsCanChangeParent" Value="False"/>
                <Setter Property="ItemsCanAttachConnectorEndPoint" Value="False"/>
                <Setter Property="ItemsCanAttachConnectorBeginPoint" Value="False"/>
                <Setter Property="ItemsCanSelect" Value="False"/>
                <Setter Property="ItemsCanEdit" Value="False"/>
                <Setter Property="ItemsCanCopyWithoutParent" Value="False"/>
                <Setter Property="ItemsCanMove" Value="False"/>
                <Setter Property="ItemsCanRotate" Value="False"/>
                <Setter Property="ItemsCanSnapToOtherItems" Value="False"/>
                <Setter Property="ItemsCanDeleteWithoutParent" Value="False"/>
                <Setter Property="ItemsCanSnapToThisItem" Value="False"/>
                <Setter Property="ItemsCanResize" Value="False"/>
                <Setter Property="Shape" Value="StandardContainers.Plain"/>
                <Setter Property="ThemeStyleId" Value="Variant2"/>
            </Style>
        </ResourceDictionary>
    </local:DiagramDemoModule.Resources>
    <DockPanel>
