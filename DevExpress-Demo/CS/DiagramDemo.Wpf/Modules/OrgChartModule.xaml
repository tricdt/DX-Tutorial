                        <dxb:BarEditItem.EditSettings>
                            <dxe:CheckEditSettings Content="{dxdiag:DiagramControlLocalizedString ShowGrid}" />
                        </dxb:BarEditItem.EditSettings>
                    </dxb:BarEditItem>
                </dxr:RibbonPageGroup>
