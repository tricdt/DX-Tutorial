                    <dxb:BarButtonItem RibbonStyle="SmallWithText"
                                       Command="{Binding Path=Commands.FitToDrawing, ElementName=diagramControl}"
                                       Style="{DynamicResource {dxdiagt:DesignerControlCommandsThemeKeys ResourceKey=FitToDrawingCommandItemStyle}}" />
                    <dxb:BarEditItem EditValue="{Binding ShowGrid, ElementName=diagramControl}"
