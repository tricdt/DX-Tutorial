        Private Sub AddEvent(ByVal events As System.Collections.Generic.IList(Of SchedulingDemo.EventData), ByVal startDate As System.DateTime, ByVal endDate As System.DateTime, ByVal subject As String, ByVal description As String, ByVal allDay As Boolean, ByVal resourceIds As String, ByVal eventType As Integer, ByVal location As String, ByVal labelId As Integer, ByVal statusId As Integer)
            Dim result As SchedulingDemo.EventData = New SchedulingDemo.EventData()
            result.StartDate = startDate
            result.EndDate = endDate
            result.Subject = subject
            result.Description = description
            result.AllDay = allDay
            result.CalendarIds = resourceIds
            result.EventType = eventType
            result.Location = location
            result.LabelId = labelId + 1
            result.StatusId = statusId
            events.Add(result)
        End Sub
