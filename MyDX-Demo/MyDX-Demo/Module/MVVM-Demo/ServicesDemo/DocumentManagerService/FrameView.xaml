<UserControl x:Class="MyDX_Demo.Module.MVVM_Demo.ServicesDemo.DocumentManagerService.FrameView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MyDX_Demo.Module.MVVM_Demo.ServicesDemo.DocumentManagerService"
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
             xmlns:dxwuin="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/navigation"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <dxwui:NavigationFrame AnimationType="SlideHorizontal">
        <dxwui:NavigationFrame.Source>
            <local:CollectionView>
                <dxmvvm:Interaction.Behaviors>
                    <dxwuin:FrameDocumentUIService/>
                </dxmvvm:Interaction.Behaviors>
            </local:CollectionView>
        </dxwui:NavigationFrame.Source>
    </dxwui:NavigationFrame>
</UserControl>
